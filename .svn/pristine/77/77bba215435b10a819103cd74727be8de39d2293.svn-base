.wdFetch-page {
  padding-bottom: 62px;
  background-color: #fff;
}

.wdFetch-page
.nav::after {
  display: none;
}

.wdFetch-page .fixed {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
    width: 100%;
    background-color: #fff;
}

.wdFetch-page
.list {
  overflow: hidden;
  width: 100%;
}

.wdFetch-page
.list__item {
  overflow: hidden;
  padding: 0 0 0 12px;
  font-size: 15px;
}

.wdFetch-page
.list__item::after {
 position: absolute;
 left: 12px;
 bottom: 0;
}

.wdFetch-page
.list__item label {
  display: inline-block;
  float: left;
  width: 75px;
  color: #8e8e93;
}

.wdFetch-page
.list__item .input-wrapper {
  margin-left: 85px;
}

.wdFetch-page
.list__item input {
  width: 100%;
  height: 30px;
  margin-bottom: 0;
  border: none;
  color: #000;
  font-size: 15px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-tap-highlight-color: transparent;

}

.wdFetch-page 
.item-body 
.pull-right .btn {
  margin-left: 9px;
  color: #6e6e6e;
  border-color: #6e6e6e;
  vertical-align: top;
}

.wdFetch-page
.fixed .btn {
    display: block;
    margin: 9px;
    background-color: #32beff;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}