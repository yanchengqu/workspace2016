.item-page {
  background-color: #fff;
}

.item-page
.nav::after {
  display: none;
}

.item-page 
.btn-important {
  background-color: #f02864;
}

.item-page
.subnav {
  margin-bottom: 0;
}

.item-page
.subnav .item-head::after {
  display: none;
}

.item-page
.subnav .item-head__inner {
  position: relative;
  height: 40px;
  line-height: 20px;
  padding: 7px 12px;
}

.item-page
.subnav .item-head__inner 
.clearfix {
  position: relative;
}

.item-page
.subnav .item-head__inner
.pull-left {
  width: 100%;
  padding-right: 90px;
  color: #000;
  font-size: 14px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.item-page
.subnav .item-head__inner
.pull-left div {
  overflow: hidden;
  white-space: nowrap;
  word-wrap: normal;
  text-overflow: ellipsis;
}

.item-page
.subnav .item-head__inner
.pull-left p {
  color: #f02864;
}

.item-page
.subnav .item-head__inner
.btn {
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -15px;
}

.item-page
.subnav .item-head__inner
[class*="fes-"] {
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  bottom: 0;
}

.item-page
.subnav .item-head__inner
.fes-chevron-down::before,
.item-page
.subnav .item-head__inner
.fes-chevron-down::before {
  vertical-align: top;
}

.item-page
.subnav .item-body {
  overflow-x: scroll;
  font-size: 0;
  white-space: nowrap;
  word-wrap: normal;
  -webkit-overflow-scrolling: touch;
}

.item-page
.subnav 
.item-body::-webkit-scrollbar-track-piece {
  border-radius: 3px;
  -webkit-border-radius: 3px;
  -ms-border-radius: 3px;
  border-radius: 3px;
}

.item-page
.subnav 
.item-body::-webkit-scrollbar {
    width: 100%;
    height: 3px;
}

.item-page
.subnav 
.item-body::-webkit-scrollbar-thumb:horizontal {
    height: 3px;
    border-radius: 3px;
    -webkit-border-radius: 3px;
    -ms-border-radius:3px;
    background-color: #efefef;
}

.item-page
.subnav .item-body
a {
  display: inline-block;
  width: 109px;
  margin-right: 20px;
  color: #6e6e6e;
  font-size: 12px;
  text-align: center;
  text-decoration: none;
}

.item-page
.subnav .item-body
a:last-child {
  margin-right: 0;
}

.item-page
.subnav .item-body
a img {
  display: block;
  width: 109px;
  height: 61px;
  margin-bottom: 8px;
  vertical-align: top;
}


.item-page
.promos {

}

.item-page
.promos img{
  width: 100%;
  height: auto;
  vertical-align: top;
}

.item-page
.bar {
  position: relative;
  overflow: hidden;
  line-height: 20px;
  padding: 10px 0;
}

.item-page
.bar::before {
  position: absolute;
  left: 0;
  top: 0;
  clear: both;
}

.item-page
.bar::after {
  position: absolute;
  left: 0;
  bottom: 0;
}

.item-page .bar 
.list__item {
  float: left;
  width: 25%;
  padding-left: 0;
  color: #6d6e71;
  font-size: 15px;
  text-align: center;
}

.item-page .bar 
.list__item::after {
  display: none;
}

.item-page
.bar a {
  color: #6e6e6e;
  text-decoration: none;
}

.item-page
.bar .cur {
  color: #f02864;
}

.item-page
.rate { margin-bottom: 0; }

.item-page
.rate::before,
.item-page
.rate::after {
  display: none;
}

.item-page 
.rate .item-body {
  padding: 30px 0;
}

.item-page
.rate .item-body__aside {
  position: relative;
  width: 100px;
  height: 86px;
  margin-right: -100px;
  text-align: center;
}

.item-page
.rate .item-body__aside div {
  margin: auto;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  height: 50px;
  line-height: 20px;
  color: #6d6e71;
  font-size: 14px;
}

.item-page
.rate .item-body__aside span {
  display: block;
  line-height: 30px;
  color: #f02864;
  font-size: 25px;
}

.item-page
.rate .item-body__aside::after {
  position: absolute;
  right: 0;
  top: 0;
  content: '';
  width: 1px;
  height: 100%;
  background-color: #efefef;
}

.item-page
.rate .item-body__main {
  overflow: hidden;
  position: relative;
  width: 100%;
  height: 86px;
  margin: 0;
}

.item-page
.rate .item-body__main .clearfix {
  line-height: 21px;
  padding: 0 12px 0 120px;
}

.item-page
.rate .item-body__main .pull-left {
  width: 80px;
  color: #6d6e71;
  font-size: 14px;
}

.item-page
.rate .item-body__main .pull-right {
  float: none;
  margin-left: 80px;
}


.item-page
.progressbar {
  display: inline-block;
  width: 100%;
  height: 8px;
  background-color: #efefef;
  vertical-align: middle;
}

.item-page
.progressbar div {
  height: 100%;
  background-color: #f02864;
}


.item-page
.comment
.item::before,
.item-page
.comment
.item::after {
  display: none;
}

.item-page
.comment
.item
.item::after {
  display: block;
  margin-top: 10px;
}

.item-page
.comment
.item {
  margin-bottom: 20px;
}


.item-page
.comment
.item:last-child {
  margin-bottom: 0;
}

.item-page
.comment
.item-body {
  padding: 0 12px;
}

.item-page
.comment
.item .item
.item-body {
  padding: 0px;
}

.item-page
.comment
.item-body__aside {
  width: 43px;
  height: 43px;
}

.item-page
.comment
.item-body__aside img {
  width: 100%;
  height: 100%;
  vertical-align: middle;
  webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}

.item-page
.comment
.item-body__main {
  margin-left: 50px;
}

.item-page
.comment
.item-body__top {
  font-size: 0;
}

.item-page
.comment
.item-body__top div {
  color: #000;
  font-size: 15px;
}

.item-page
.comment
.item-body__top p {
  line-height: 18px;
  margin: 10px 0;
  color: #000;
  font-size: 14px;
}

.item-page
.comment
.item-body__top img {
  width: 55px;
  height: 55px;
  margin-bottom: 10px;
  margin-right: 10px;
  vertical-align: top;
}

.item-page
.comment
.item-body__top
.clearfix div {
  color: #6d6e71;
  font-size: 12px;
}

.item-page
.comment
.item-body__top
.pull-right::before {
  display: inline-block;
  content: '';
  width: 12px;
  height: 9px;
  background-image: url(data:image/png;base64,);
  -webkit-background-size: 12px auto;
  background-size: 12px auto;
  background-repeat: no-repeat;
}

.item-page
.param {
  overflow: hidden;
  margin-top: -1px;
  color: #6d6e71;
  font-size: 15px;
}

.item-page
.param::after {
  display: none;
}

.item-page
.param
.list__item {
  position: relative;
  float: left;
  width: 50%;
  padding-left: 0;
  text-indent: 12px;
  white-space: nowrap;
}

.item-page
.param
.list__item:last-child::after {
  display: block;
}

.item-page
.param
.list__item:nth-child(odd)::before {
  position: absolute;
  right: 0;
  top: 0;
  content: '';
  width: 1px;
  height: 100%;
  background-color: #efefef;
}

@media screen and (-webkit-min-device-pixel-ratio: 2) {
  .item-page
  .param
  .list__item::before,
  .item-page
  .data .pull-left::after {
      -webkit-transform: scaleX(0.5);
      transform: scaleX(0.5);
  }

}

.item-page
.viewMore {
	padding: 12px 20px;
}

.item-page
.viewMore
.btn-line {
	display: block;
	border-color: #d7d7d7;
    color: #6e6e6e;
}

.item-page
.viewMoreloading {
	display: none;
}

.item-page
.viewMoreloading
.loading {
	margin: 0 auto;
}

.item-page
.doCommentContainer
textarea {
    overflow: auto;
    width: 100%;
    min-height: 200px;
	line-height: 20px;
	padding: 0;
	margin-bottom: 0;
    font-size: 12px;
    -webkit-appearance: none;
    -webkit-tap-highlight-color: transparent;
    -moz-tap-highlight-color: transparent;
    -ms-tap-highlight-color: transparent;
    -o-tap-highlight-color: transparent;
}

.item-page 
.nav__share{
  background-image: url('http://static.tongshuai.com/wd/img/pages/item/ic_Share.png');  
  background-size: 20px 20px;
  background-position: center center;
  background-repeat: no-repeat;
  width: 44px;
  height: 44px;
  position: relative;
  float: right;
  display: inline-block;
}