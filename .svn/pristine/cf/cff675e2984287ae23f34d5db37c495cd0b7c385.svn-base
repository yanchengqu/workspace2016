	border: none;
}

.order-submit-page .order-submit {
	padding: 0 0 60px;
	text-align: right;
}
.order-submit-page .btn-large {
	padding: 8px 20px;
	border-radius: 4px;
	font-family: "Microsoft YaHei";

}
.order-submit-page .order--amount {
	margin-right: 50px;
	font-size: 18px;
}
.order-submit-page .order--amount em {
	color: #ed1b23;
}


