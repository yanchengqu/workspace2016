.consignee-manage .consignee--name,
.consignee-manage .consignee--tel,
.consignee-manage .consignee--email,
.consignee-manage .consignee--province,
.consignee-manage .consignee--city ,
.consignee-manage .consignee--region,
.consignee-manage .consignee--address {
