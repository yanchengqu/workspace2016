.adm-page {
  padding-bottom: 62px;
  background-color: #f4f4f4;
}

.adm-page
.nav::after {
  display: none;
}

.adm-page .fixed {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
    width: 100%;
    background-color: #fff;
}

.adm-page 
.item-head__inner {
  line-height: 25px;
  padding: 12px;
  color: #000;
  font-size: 15px;
  font-weight: bold;
}

.adm-page 
.item-head 
.pull-left {

}

.adm-page 
.item-head 
.pull-right {

}

.adm-page 
.item-head p {
  clear: both;
  line-height: 20px;
  font-size: 14px;
  font-weight: normal;
}

.adm-page 
.item-body {
  height: 30px;
  line-height: 30px;
  padding: 9px 12px;
  font-size: 14px;
}

.adm-page 
.item-body 
.pull-left {
  color: #6e6e6e;
}

.adm-page 
.item-body 
.checkbox {
  position: relative;
  top: 6px;
  margin-right: 8px;
  vertical-align: top;
}

.adm-page 
.item-body 
.pull-right .btn {
  margin-left: 9px;
  color: #6e6e6e;
  border-color: #6e6e6e;
  vertical-align: top;
}

.adm-page
.fixed .btn {
    display: block;
    margin: 9px;
    background-color: #32beff;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}