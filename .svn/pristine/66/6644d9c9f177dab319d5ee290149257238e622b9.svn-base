[class^="fes-"],
[class*="fes-"] {
  position: relative;
  display: inline-block;
  overflow: hidden;
  width: 25px;
  height: 25px;
  line-height: 25px;
}

.fes-close{
  color: currentcolor;
  font-size: 25px;
  font-family: icon;
  font-style: normal;
  text-align: center;
}

.fes-close:before {
 position: absolute;
 display: block;
 vertical-align: middle;
 content: '\00d7';
}

.fes-ok {
  display: inline-block;
  overflow: hidden;
  width: 5px;
  height: 10px;
  border-right: 2px solid currentcolor;
  border-bottom: 2px solid currentcolor;
  -webkit-transform: rotate(40deg);
  -ms-transform: rotate(40deg);
  -o-transform: rotate(40deg);
  transform: rotate(40deg);
}

[class^="fes-chevron-"]:before,
[class*="fes-chevron-"]:before {
  position: absolute;
  display: block;
  content: '';
  width: 12px;
  height: 12px;
  border-left: 1px solid currentcolor;
  border-bottom: 1px solid currentcolor;

}
.fes-chevron-left:before {
  top: 6px;
  left: 9px;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}

.fes-chevron-right:before {
  top: 6px;
  left: 3px;
  -webkit-transform: rotate(-135deg);
  -ms-transform: rotate(-135deg);
  -o-transform: rotate(-135deg);
  transform: rotate(-135deg);
}
.fes-chevron-up:before {
  top: 9px;
  left: 6px;
  -webkit-transform: rotate(135deg);
  -ms-transform: rotate(135deg);
  -o-transform: rotate(135deg);
  transform: rotate(135deg);
}
.fes-chevron-down:before {
  top: 1px;
  left: 6px;
  -webkit-transform: rotate(-135deg);
  -ms-transform: rotate(-135deg);
  -o-transform: rotate(-135deg);
  transform: rotate(-45deg);
}

