.feedback-page {
  /* padding-bottom: 50px; */
  background-color: #fff;
}

.feedback-page
.tips {
	line-height: 20px;
	padding: 22px 12px 18px;
	background-color: #f4f4f4;
	color: #000;
	font-size: 15px;
	text-align: center;
}

.feedback-page
.tips a {
	color: #32beff;
	font-size: 17px;
	text-decoration: none;
}
.feedback-page .wrapper{
  overflow: hidden;
  padding: 0 12px;
}


.feedback-page
.item::before,
.item::after {
	display: none;
}

.feedback-page
.item-body {

	padding: 0;
	color: #000;
	font-size: 15px;
}

.feedback-page
.list__item {
  overflow: hidden;
  padding: 0 0;
}

.list::before, 
.list::after {
  display: none;
}

.feedback-page 
.list__item::after {
  display: none;
}

.feedback-page
.list__item .pull-left {
	width: 4em;
	font-size: 15px;
    white-space: nowrap;
    word-wrap: normal;
    word-break: normal;
}

.feedback-page
.list__item .input-wrapper {
  margin-left: 5em;
}

.feedback-page
.list__item input {
  width: 100%;
  height: 30px;
  border: none;
  margin-bottom: 0;
  color: #000;
  font-size: 15px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-tap-highlight-color: transparent;
}

.feedback-page 
.btn {
  display: block;
  background-color: #32beff;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
}

.feedback-page 
.btn {
  margin-bottom: 10px;
}

.feedback-page
.wrapper
.text-right a {
  color: #6d6e71;
  font-size: 14px;
  text-decoration: none;
}

.feedback-page
.otherLogin {
  padding: 0 12px;
}

.feedback-page
.otherLogin div {
  position: relative;
  text-align: center;
}

.feedback-page
.otherLogin 
div::after {
  position: absolute;
  left: 0;
  top: 50%;
  z-index: -1;
  display: block;
  content: '';
  width: 100%;
  height: 1px;
  background-color: #efefef;
}

@media screen and (-webkit-min-device-pixel-ratio: 2) {
  .feedback-page .otherLogin div::after {
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
  }
}

.feedback-page
.otherLogin span {
  padding: 0 14px;
  color: #6d6e71;
  font-size: 14px;
  background-color: #fff;
}

.feedback-page
.otherLogin ul {
  padding: 20px 0;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  display: flex;
}

.feedback-page
.otherLogin li {
  text-align: center;
  -webkit-flex: 1;
  -moz-flex: 1;
  -ms-flex: 1;
  -o-flex: 1;
  flex: 1;
}

.feedback-page
.otherLogin img {
  width: 51px;
  height: auto;
}
