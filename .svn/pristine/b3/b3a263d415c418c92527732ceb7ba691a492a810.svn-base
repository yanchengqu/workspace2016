	<script id="orderListScriptTemplate" type="text/template">
		<%_.each(data, function(item) {%>
            <div class="list__item--body order-goods">
                <div class="order-thead clearfix">
                    <div class="order-list__num">&nbsp;&nbsp;订单号：<%=item.orderSn%></div>
                    <div class="order-list__state mark-red"><%=item.orderStatusName%></div>
                </div>
                <%_.each(item.showOrdProductListVo, function(jitem) {%>
            	    <table><tbody><tr>
                        <td width="11%">
                            <a href="#">
                                <img class="order-goods__img" src="<%=jitem.imgFileId%>" alt="">
                            </a>
                         </td>
                         <td width="29%">
                            <a href="/pc/product/<%=jitem.sku%>.html">
                                <span class="order-goods__des"><%=jitem.productName%></span>
                            </a>
                        </td>
                        <td class="text-center" width="15%">
                            <div class="order-goods--price"><em><%=jitem.price%></em></div>
                        </td>
                        <td class="text-center" width="15%">
                            <div class="order-goods--count">数量：<em><%=jitem.productNumber%></em></div>
                        </td>
                        <td class="text-center" width="15%">
                            <div class="order-goods--total">小计：<em><%=item.paidAmount%></em></div>
                        </td>
                        <td class="text-center" width="15%">
                            <div class="list__item--operate">
                            	<%if(item.isCod == 0 && item.orderStatus == 200 && item.paymentStatus == 100){%>
									<a href="javascript:void(0);" 
										onclick="paymentOrder('<%=item.payUrl%>')">支付订单</a><br>
								<%}%>
								<%if(item.orderSyncToCbsStatus == 1){%>
									<a href="javascript:void(0);" 
										onclick="trackOrder('<%=item.orderSn%>')" >追踪订单</a><br>
								<%}%>
								<%if(item.isCod == 0 && item.orderStatus == 200 && item.paymentStatus == 100){%>
									<a href="javascript:void(0);" 
										onclick="cancelOrder('<%=item.orderSn%>')">取消订单</a><br>
								<%}else if(item.isCod == 1 && item.orderStatus == 200){%>
									<a href="javascript:void(0);" 
										onclick="cancelOrder('<%=item.orderSn%>')">取消订单</a>
								<%}%>
								<%if(item.orderStatus == 202 || item.orderStatus == 203){%>
									<a href="/pc/product_buy/<%=jitem.productId%>.html" >再次购买</a><br>
									<a href="javascript:void(0);"
										onclick="deleteOrderConfirm('<%=item.orderSn%>')">删除</a>
								<%}%>
                            </div>
                        </td>
                    </tr></tbody></table>
        		<%});%>
			</div>
		<%});%>
	</script>
